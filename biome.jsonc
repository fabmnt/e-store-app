{
  "$schema": "https://biomejs.dev/schemas/2.2.0/schema.json",
  "extends": ["ultracite"],
  "linter": {
    "rules": {
      "performance": {
        "noNamespaceImport": "off"
      },
      "suspicious": {
        "noConsole": "off"
      },
      "correctness": {
        "noChildrenProp": "off"
      },
      "a11y": {
        "useSemanticElements": "off"
      }
    }
  }
}
